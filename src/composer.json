{
    "name": "paulk/zicht",
    "description": "Opdracht senior backend developer Zicht",
    "require": {
        "illuminate/database": "~5.5",
        "monolog/monolog": "^1.23",
        "slim/slim": "^3.0",
        "vlucas/phpdotenv": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
